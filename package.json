    "pg-query-stream": "^1.0.0",
    "pm2": "^1.1.1",
    "qtip2": "3.0.2",
    "redis": "2.5.3",
    "request": "2.72.0",
    "request-progress": "2.0.1",
    "select2": "^4.0.2",
    "select2-bootstrap-theme": "0.1.0-beta.4",
